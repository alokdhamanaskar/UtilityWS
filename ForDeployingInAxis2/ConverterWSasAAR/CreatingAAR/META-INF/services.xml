<?xml version="1.0" encoding="UTF-8"?>
<serviceGroup>
    <service name="converters" scope="application">
        <description>The converter Web service has some general purpose utility operations like decoders/encoders for base64Binary, CSV to array and vice versa </description>
        <messageReceivers>
            <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only" class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver"/>
            <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out" class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
        </messageReceivers>
        <parameter name="ServiceClass">converters.GeneralPurposeConverters</parameter>
    </service>
</serviceGroup>
